let user = {
  name: 'aan',
  age: 23,
  data: {
    username: 'aan',
    friends: [
      'fahmi',
      'yani',
      'ira'
    ],
    location: {
      city: 'tandun',
      state: 'riau',
    }
  }
};

===============================================================


for(let i in user){
  console.log(i);    //name, age, data
}




