var b=["1", "2", "3", "5", "6"]

var c=b.splice(3,0, "4")  // 3- index
                          // 0- item remove
                          // "4"- item ditambah

====================================================================
  console.log(b)            // [ '1', '2', '3', '4', '5', '6' ]
  console.log(c)            // []


---------------------------------------------------------------------
// ternyata,, splice digunakan untuk input nilai ditengah list/ array
---------------------------------------------------------------------











//////////////////////////////////////////////////////////////////////

var b=["1", "2", "3", "5", "6"]

var c=b.splice(3,2, "4")  // 3- index
                          // 2- item remove
                          // "4"- item ditambah
====================================================================
console.log(b)            // [ '1', '2', '3', '4' ]
console.log(c)            // [[ '5', '6' ]
